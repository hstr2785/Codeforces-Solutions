    def insearch():
           n=int(input())
           l=list()
           s=0
           ni=input()
           for i in ni.split(" "):
                  l.append(int(i))
           for i in l:
                  if(i==1):
                      print("Hard")
                      break
                  else:
                          s=s+1
           if s==n:
               print("Easy")
    insearch() 
